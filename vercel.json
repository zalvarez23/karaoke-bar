{
  "functions": {
    "api/youtube/search-youtube.ts": {
      "runtime": "nodejs18.x"
    },
    "api/youtube/suggestions.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/youtube/(.*)",
      "destination": "/api/youtube/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
